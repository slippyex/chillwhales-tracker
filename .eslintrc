{
    "parser": "@typescript-eslint/parser",
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "env": {
        "browser": false,
        "es6": true,
        "node": true
    },
    "rules": {
        "no-console": "off",
        "@typescript-eslint/explicit-module-boundary-types": "off"
    }
}
